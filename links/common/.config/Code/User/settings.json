{
    "editor.bracketPairColorization.enabled": true,
    "remote.SSH.remotePlatform": {
        "LocalNeo": "linux",
        "RemoteNeo": "linux",
        "AwsFallDetect": "linux"
    },
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.enableNeovim": true,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "d"],
            "after": ["d", "d"]
        },
        {
            "before": ["<C-n>"],
            "commands": [":nohl"]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.zip": "default"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.codeActionsOnSave": {
        "source.fixAll.markdownlint": true
    },
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "tabnine.experimentalAutoImports": true,
    "settingsSync.ignoredExtensions": [
        "ms-vscode-remote.remote-wsl",
        "serayuzgur.crates"
    ],
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorTheme": "One Dark Pro Darker",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "jupyter.askForKernelRestart": false,
    "vim.neovimUseConfigFile": true,
    "jupyter.generateSVGPlots": true,
    "files.watcherExclude": {
        "**/target/**": true,
        "**/venv/**": true
    },
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.profiles.linux": {},
    "terminal.integrated.localEchoExcludePrograms": [
        "vim",
        "vi",
        "nano",
        "tmux",
        "nvim",
        "hx"
    ],
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.languageServer": "Pylance",
    "redhat.telemetry.enabled": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "explorer.confirmDragAndDrop": false,
    "crates.listPreReleases": true,
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.tabWidth": 4,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "python.defaultInterpreterPath": "C:\\Users\\micro\\AppData\\Local\\Programs\\Python\\Python310\\python.exe",
    "[sql]": {
        "editor.formatOnSave": false
    },
    "sqltools.queryParams.enableReplace": true,
    "sqltools.queryParams.regex": "s=>",
    "workbench.startupEditor": "none",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "git.confirmSync": false,
    "git.autofetch": true,
    "explorer.confirmDelete": false,
    "xmlTools.enforcePrettySelfClosingTagOnFormat": true,
    "xmlTools.splitXmlnsOnFormat": false,
    "sqltools.highlightQuery": false,
